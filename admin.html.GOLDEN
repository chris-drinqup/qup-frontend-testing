        // Helper function for rendering values
        function renderValue(value, key) {
            key = key || '';
            
            if (value === null) return '<span style="color: var(--text-secondary);">null</span>';
            if (value === undefined) return '<span style="color: var(--text-secondary);">undefined</span>';
            if (typeof value === 'boolean') return '<span style="color: var(--success);">' + value + '</span>';
            if (typeof value === 'number') return '<span style="color: var(--info);">' + value + '</span>';
            if (typeof value === 'string') {
                // Escape HTML characters to prevent syntax issues
                var escapedValue = String(value)
                    .replace(/&/g, '&amp;')
                    .replace(/</g, '&lt;')
                    .replace(/>/g, '&gt;')
                    .replace(/"/g, '&quot;')
                    .replace(/'/g, '&#39;');
                return '<span style="color: var(--warning);">"' + escapedValue + '"</span>';
            }
            if (Array.isArray(value)) return '<span style="color: var(--text-secondary);">[Array: ' + value.length + ' items]</span>';
            if (typeof value === 'object') return '<span style="color: var(--text-secondary);">{Object: ' + Object.keys(value).length + ' props}</span>';
            
            // Fallback for any other type - escape it
            var fallbackValue = String(value)
                .replace(/&/g, '&amp;')
                .replace(/</g, '&lt;')
                .replace(/>/g, '&gt;')
                .replace(/"/g, '&quot;')
                .replace(/'/g, '&#39;');
            return '<span style="color: var(--text-secondary);">' + fallbackValue + '</span>';
        }

        function renderVariableList(obj, category) {
            if (!obj || typeof obj !== 'object') {
                return '<div style="color: var(--text-secondary); font-style: italic;">No data available</div>';
            }

            // Handle error objects
            if (obj.error) {
                var errorMsg = String(obj.error)
                    .replace(/&/g, '&amp;')
                    .replace(/</g, '&lt;')
                    .replace(/>/g, '&gt;')
                    .replace(/"/g, '&quot;')
                    .replace(/'/g, '&#39;');
                return '<div style="color: var(--danger); font-style: italic;">Error: ' + errorMsg + '</div>';
            }

            var html = '';
            
            function processObject(obj, prefix) {
                prefix = prefix || '';
                
                try {
                    var entries = Object.entries(obj);
                    for (var i = 0; i < entries.length; i++) {
                        var entry = entries[i];
                        var key = entry[0];
                        var value = entry[1];
                        var fullKey = prefix ? prefix + '.' + key : key;
                        
                        // Escape the key to prevent HTML injection
                        var safeKey = String(key)
                            .replace(/&/g, '&amp;')
                            .replace(/</g, '&lt;')
                            .replace(/>/g, '&gt;')
                            .replace(/"/g, '&quot;')
                            .replace(/'/g, '&#39;');
                        
                        if (typeof value === 'object' && value !== null && !Array.isArray(value) && Object.keys(value).length < 10) {
                            var paddingLeft = prefix ? '20px' : '0px';
                            html += '<div style="margin-bottom: 8px; padding-left: ' + paddingLeft + ';">';
                            html += '<div style="color: var(--text-primary); font-weight: 600; margin-bottom: 4px;">' + safeKey + ':</div>';
                            html += '<div style="margin-left: 15px;">';
                            processObject(value, fullKey);
                            html += '</div></div>';
                        } else {
                            var paddingLeft = prefix ? '20px' : '0px';
                            html += '<div style="display: flex; justify-content: space-between; align-items: center; padding: 4px 0; border-bottom: 1px solid var(--border); margin-left: ' + paddingLeft + ';">';
                            html += '<span style="color: var(--text-primary); font-weight: 600; font-size: 0.9rem;">' + safeKey + '</span>';
                            html += '<span style="font-family: Courier New, monospace; font-size: 0.85rem;">' + renderValue(value, key) + '</span>';
                            html += '</div>';
                        }
                    }
                } catch (error) {
                    html += '<div style="color: var(--danger); font-style: italic;">Error processing object: ' + error.message + '</div>';
                }
            }

            processObject(obj);
            
            return html || '<div style="color: var(--text-secondary); font-style: italic;">No variables found</div>';
        }

        function renderMonitorDisplay(data) {
            try {
                var html = '';
                html += '<div style="margin-bottom: 20px; text-align: center; color: var(--text-secondary);">';
                html += '<span style="color: var(--success);">‚óè</span> Live System Monitor - Last Updated: ';
                html += new Date().toLocaleTimeString();
                html += '</div>';

                html += '<div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(0px, 1fr)); gap: 20px;">';
                
                // System Health
                html += '<div style="background: var(--bg-secondary); border: 1px solid var(--border); border-radius: 10px; padding: 20px;">';
                html += '<h4 style="color: var(--primary); margin-bottom: 15px;">System Health</h4>';
                html += renderVariableList(data.health, 'health');
                html += '</div>';

                // Queue System
                html += '<div style="background: var(--bg-secondary); border: 1px solid var(--border); border-radius: 10px; padding: 20px;">';
                html += '<h4 style="color: var(--primary); margin-bottom: 15px;">Queue System</h4>';
                html += renderVariableList(data.queue, 'queue');
                html += '</div>';

                // WebSocket
                html += '<div style="background: var(--bg-secondary); border: 1px solid var(--border); border-radius: 10px; padding: 20px;">';
                html += '<h4 style="color: var(--primary); margin-bottom: 15px;">WebSocket</h4>';
                html += renderVariableList(data.websocket, 'websocket');
                html += '</div>';

                // Configuration
                html += '<div style="background: var(--bg-secondary); border: 1px solid var(--border); border-radius: 10px; padding: 20px;">';
                html += '<h4 style="color: var(--primary); margin-bottom: 15px;">Configuration</h4>';
                html += renderVariableList(data.config, 'config');
                html += '</div>';

                // Client State
                html += '<div style="background: var(--bg-secondary); border: 1px solid var(--border); border-radius: 10px; padding: 20px;">';
                html += '<h4 style="color: var(--primary); margin-bottom: 15px;">Client State</h4>';
                html += renderVariableList(data.client_state, 'client');
                html += '</div>';

                // Time Management (if available)
                if (data.queue && data.queue.time_management) {
                    html += '<div style="background: var(--bg-secondary); border: 1px solid var(--border); border-radius: 10px; padding: 20px;">';
                    html += '<h4 style="color: var(--primary); margin-bottom: 15px;">Time Management</h4>';
                    html += renderVariableList(data.queue.time_management, 'time');
                    html += '</div>';
                }

                html += '</div>';

                // Raw JSON Data Section
                html += '<div style="margin-top: 30px;">';
                html += '<h4 style="color: var(--primary); margin-bottom: 15px;">Raw JSON Data</h4>';
                html += '<div style="background: var(--bg-primary); border: 1px solid var(--border); border-radius: 8px; padding: 15px; max-height: 400px; overflow-y: auto;">';
                html += '<pre style="color: var(--text-secondary); font-size: 0.85rem; white-space: pre-wrap; margin: 0;">';
                
                // Safely escape JSON data
                var jsonString = JSON.stringify(data, null, 2)
                    .replace(/&/g, '&amp;')
                    .replace(/</g, '&lt;')
                    .replace(/>/g, '&gt;')
                    .replace(/"/g, '&quot;');
                html += jsonString;
                
                html += '</pre>';
                html += '</div>';
                html += '</div>';

                document.getElementById('monitorContent').innerHTML = html;
                
            } catch (error) {
                console.error('Error rendering monitor display:', error);
                document.getElementById('monitorContent').innerHTML = 
                    '<div class="alert alert-error">Failed to render monitor data: ' + error.message + '</div>';
            }
        }

        function exportMonitorData() {
            if (!systemData || !Object.keys(systemData).length) {
                showAlert('No monitor data to export. Please refresh first.', 'error');
                return;
            }

            const blob = new Blob([JSON.stringify(systemData, null, 2)], { type: 'application/json' });
            const url = URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = `qup-system-variables-${new Date().toISOString().split('T')[0]}.json`;
            a.click();
            URL.revokeObjectURL(url);
            showAlert('System variables exported successfully', 'success');
        }

        // Export functions
        function exportQueue() {
            apiCall('/api/v1/queue/list?admin=true')
                .then(response => response.json())
                .then(data => {
                    const blob = new Blob([JSON.stringify(data, null, 2)], { type: 'application/json' });
                    const url = URL.createObjectURL(blob);
                    const a = document.createElement('a');
                    a.href = url;
                    a.download = `qup-queue-${new Date().toISOString().split('T')[0]}.json`;
                    a.click();
                    URL.revokeObjectURL(url);
                    showAlert('Queue exported successfully', 'success');
                })
                .catch(error => {
                    showAlert('Failed to export queue: ' + error.message, 'error');
                });
        }

        function clearCompleted() {
            if (confirm('Clear all completed orders?')) {
                showAlert('Clear completed functionality coming soon...', 'info');
            }
        }

        // API helper
        async function apiCall(endpoint, options = {}) {
            if (!adminKey) {
                throw new Error('Admin key not available');
            }

            const defaultOptions = {
                headers: {
                    'Content-Type': 'application/json',
                    'X-Admin-Key': adminKey
                }
            };

            return fetch(API_BASE + endpoint, { ...defaultOptions, ...options });
        }

        // Utility functions
        function showAlert(message, type = 'info') {
            const container = document.getElementById('alertsContainer');
            const alert = document.createElement('div');
            alert.className = `alert alert-${type}`;
            alert.textContent = message;

            container.appendChild(alert);

            setTimeout(() => {
                if (alert.parentNode) {
                    alert.remove();
                }
            }, 5000);
        }

        function setLoading(type, loading) {
            const text = document.getElementById(type + 'Text');
            const spinner = document.getElementById(type + 'Loading');

            if (loading) {
                text.style.display = 'none';
                spinner.classList.remove('hidden');
            } else {
                text.style.display = 'inline';
                spinner.classList.add('hidden');
            }
        }

        // Initialize when DOM is ready
        document.addEventListener('DOMContentLoaded', init);
    </script>
</body>
</html>
